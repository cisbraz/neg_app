  object qryNegociacao: TFDQuery
    Connection = dtmBanco.cConexao
    SQL.Strings = (
      'SELECT CN.NEGOCIACAO_ID,'
      '       PD.RAZAO_SOCIAL AS NOME_PRODUTOR,'
      '       DI.RAZAO_SOCIAL AS NOME_DISTRIBUIDOR,'
      '       CN.DT_CADASTRO,'
      '       CN.DT_APROVACAO,'
      '       CN.DT_CANCELAMENTO,'
      '       CN.DT_CONCLUSAO,'
      
        '       (SELECT COALESCE(SUM(IT.QUANTIDADE * IT.PRECO_UNITARIO),0' +
        ') '
      '          FROM ITENS_NEGOCIACAO IT'
      
        '         WHERE IT.NEGOCIACAO_ID = CN.NEGOCIACAO_ID) AS TOTAL_NEG' +
        'OCIACAO,'
      '       '#39'Ok'#39' AS EDITAR,'
      '       CASE'
      '          WHEN (CN.STATUS = '#39'C'#39') THEN '#39'Cancelada'#39
      '          WHEN (CN.STATUS = '#39'A'#39') THEN '#39'Aprovada'#39
      '          WHEN (CN.STATUS = '#39'U'#39') THEN '#39'Conclu'#237'do'#39
      '          ELSE '#39'Pendente'#39
      '       END STATUS_N'
      '  FROM CAPA_NEGOCIACAO CN,'
      '       PRODUTOR PD,'
      '       DISTRIBUIDOR DI'
      ' WHERE CN.PRODUTOR_ID = PD.PRODUTOR_ID'
      '   AND CN.DISTRIBUIDOR_ID = DI.DISTRIBUIDOR_ID')
    Left = 35
    Top = 239
